{
    "name": "rougin/describe",
    "description": "Get the information about the database you're working on in PHP",
    "keywords": [
        "describe",
        "database",
        "php"
    ],
    "homepage": "https://github.com/rougin/describe",
    "license": "MIT",
    "authors": [
        {
            "name": "Rougin Royce Gutib",
            "email": "rougingutib@gmail.com",
            "homepage": "http://rougin.github.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Rougin\\Describe\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rougin\\Describe\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.5-dev"
        }
    }
}